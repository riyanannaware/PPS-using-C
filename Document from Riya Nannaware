#include<stdio.h>
int addition(a,b)
  {
	int c;
	c=a+b;
    printf("addition is %d",c);
}
    int main ()
    {
    	int n1,n2 ;
    	printf("Enter first number\t");
    	scanf("%d",&n1);
    	printf("Enter second number \t");
    	scanf("%d",&n2);
    	addition (n1,n2);
    	return 0;
	}
